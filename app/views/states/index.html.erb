<p id="notice"><%= notice %></p>

<h1>Inmuebles</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Tipo de Inmuebles</th>
      <th>Area</th>
      <th>Cantidad de Pisos</th>
      <th>Cantidad de Cuartos</th>
      <th>Direcci√≥n</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @states.each do |state| %>
      <tr>
        <td><%= state.name %></td>
        <td><%= Typeinmu.select('name').where('id = State.id') %></td>
        <td><%= state.area %></td>
        <td><%= state.countfloor %></td>
        <td><%= state.countRoom %></td>
        <td><%= state.address %></td>
        <td><%= link_to 'Ver', state %></td>
        <td><%= link_to 'Editar', edit_state_path(state) %></td>
        <td><%= link_to 'Dar de Baja', state, method: :delete, data: { confirm: 'Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
